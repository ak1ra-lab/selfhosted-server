{
    "inbounds": [
        {
            "tag": "{{ v2ray_profile }}",
            "port": {{ v2ray_vmess_quic_port }},
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "{{ v2ray_inbound_client_id }}"
                    }
                ]
            },
            "streamSettings": {
                "network": "quic",
                "security": "tls",
                "tlsSettings": {
                    "certificates": [
                        {
                            "certificateFile": "{{ v2ray_ssl_certificate }}",
                            "keyFile": "{{ v2ray_ssl_certificate_key }}"
                        }
                    ]
                },
                "quicSettings": {
                    "security": "{{ v2ray_vmess_quic_security }}",
                    "key": "{{ v2ray_vmess_quic_key }}",
                    "header": {
                        "type": "{{ v2ray_vmess_quic_header_type }}"
                    }
                }
            },
            "sniffing": {
                "enabled": true
            }
        }
    ]
}
{
    "outbounds": [
        {
            "tag": "{{ v2ray_config_profile }}",
            "protocol": "trojan",
            "settings": {
                "servers": [
                    {
                        "address": "{{ ansible_host }}",
                        "port": {{ v2ray_config_trojan_tcp_port }},
                        "password": "{{ v2ray_config_trojan_tcp_password }}"
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "tls",
                "tlsSettings": {
                    "allowInsecure": false
                }
            }
        },
        {
            "tag": "direct",
            "protocol": "freedom"
        },
        {
            "tag": "block",
            "protocol": "blackhole"
        }
    ]
}
{
    "outbounds": [
        {
            "tag": "{{ v2ray_profile }}",
            "protocol": "vmess",
            "settings": {
                "vnext": [
                    {
                        "address": "{{ ansible_host }}",
                        "port": {{ v2ray_vmess_tcp_port }},
                        "users": [
                            {
                                "id": "{{ v2ray_inbound_client_id }}",
                                "security": "auto"
                            }
                        ]
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "tls",
                "tlsSettings": {
                    "allowInsecure": false
                }
            }
        },
        {
            "tag": "direct",
            "protocol": "freedom"
        },
        {
            "tag": "block",
            "protocol": "blackhole"
        }
    ]
}
{
    "log": {
        "loglevel": "{{ v2ray_config_log_loglevel }}"
    },
    "inbounds": [
        {
            "listen": "127.0.0.1",
            "port": 1081,
            "protocol": "socks",
            "settings": {
                "udp": true
            },
            "sniffing": {
                "enabled": true
            }
        },
        {
            "listen": "127.0.0.1",
            "port": 1082,
            "protocol": "http",
            "sniffing": {
                "enabled": true
            }
        }
    ],
    "outbounds": [
        {
            "tag": "kcp",
            "protocol": "vmess",
            "settings": {
                "vnext": [
                    {
                        "address": "{{ v2ray_config_server_address }}",
                        "port": {{ v2ray_config_mkcp_port }},
                        "users": [
                            {
                                "id": "{{ v2ray_config_inbound_client_id }}",
                                "security": "auto"
                            }
                        ]
                    }
                ]
            },
            "streamSettings": {
                "network": "kcp",
                "kcpSettings": {
                    "mtu": 1350,
                    "tti": 50,
                    "uplinkCapacity": {{ v2ray_config_mkcp_uplinkCapacity }},
                    "downlinkCapacity": {{ v2ray_config_mkcp_downlinkCapacity }},
                    "congestion": {{ v2ray_config_mkcp_congestion }},
                    "readBufferSize": {{ v2ray_config_mkcp_readBufferSize }},
                    "writeBufferSize": {{ v2ray_config_mkcp_writeBufferSize }},
                    "header": {
                        "type": "{{ v2ray_config_mkcp_header_type }}"
                    },
                    "seed": "{{ v2ray_config_mkcp_seed }}"
                }
            }
        }
    ],
    "routing": {
        "domainStrategy": "AsIs",
        "domainMatcher": "mph",
        "rules": [
            {
                "type": "field",
                "ip": [
                    "geoip:private",
                    "geoip:cn"
                ],
                "outboundTag": "direct"
            },
            {
                "type": "field",
                "protocol": [
                    "bittorrent"
                ],
                "outboundTag": "direct"
            },
            {
                "type": "field",
                "domain": [
                    "geosite:category-ads"
                ],
                "outboundTag": "block"
            }
        ]
    }
}
{
    "log": {
        "loglevel": "{{ v2ray_config_log_loglevel }}",
        "access": "{{ v2ray_config_log_access }}",
        "error": "{{ v2ray_config_log_error }}"
    },
    "inbounds": [
        {
            "listen": "{{ v2ray_config_mkcp_listen }}",
            "port": {{ v2ray_config_mkcp_port }},
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "{{ v2ray_config_inbound_client_id }}"
                    }
                ]
            },
            "streamSettings": {
                "network": "kcp",
                "kcpSettings": {
                    "mtu": 1350,
                    "tti": 50,
                    "uplinkCapacity": {{ v2ray_config_mkcp_uplinkCapacity }},
                    "downlinkCapacity": {{ v2ray_config_mkcp_downlinkCapacity }},
                    "congestion": {{ v2ray_config_mkcp_congestion }},
                    "readBufferSize": {{ v2ray_config_mkcp_readBufferSize }},
                    "writeBufferSize": {{ v2ray_config_mkcp_writeBufferSize }},
                    "header": {
                        "type": "{{ v2ray_config_mkcp_header_type }}"
                    },
                    "seed": "{{ v2ray_config_mkcp_seed }}"
                }
            },
            "sniffing": {
                "enabled": true
            }
        }
    ],
    "outbounds": [
        {
            "tag": "direct",
            "protocol": "freedom"
        },
        {
            "tag": "block",
            "protocol": "blackhole"
        }
    ],
    "routing": {
        "domainStrategy": "AsIs",
        "domainMatcher": "mph",
        "rules": [
            {
                "type": "field",
                "protocol": [
                    "bittorrent"
                ],
                "outboundTag": "block"
            },
            {
                "type": "field",
                "ip": [
                    "geoip:private"
                ],
                "outboundTag": "direct"
            }
        ]
    }
}